# Example Terraform variables file
# Copy this to terraform.tfvars and customize

project_name  = "uems"
environment   = "prod"
location      = "East US"

# AKS Configuration
aks_node_count         = 2
aks_node_vm_size       = "Standard_B2s"
aks_kubernetes_version = "1.27.7"

# ACR Configuration
acr_sku = "Basic"

# Tags
tags = {
  Project     = "UEMS"
  ManagedBy   = "Terraform"
  Environment = "Production"
  Owner       = "Your Name"
}
